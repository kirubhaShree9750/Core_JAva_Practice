//https://leetcode.com/problems/find-numbers-with-even-number-of-digits/
class Solution {
    public int findNumbers(int[] nums) {

        int total_even = 0;
        for(int i =0; i<nums.length; i++){
            int num_length = (nums[i]+"").length();
            if(num_length % 2 == 0){
              total_even++;
            }
        }
        return total_even;
    }
}
